<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
  <changeSet id="21" author="kc1vmz">
    <createTable tableName="internet_server">
      <column name="internet_server_id" type="VARCHAR(36)">
        <constraints nullable="false" unique="true" primaryKey="true" primaryKeyName="internet_serverPK"/>
      </column>
      <column name="login_callsign" type="VARCHAR(20)">
        <constraints nullable="false" unique="false"/>
      </column>
      <column name="name" type="VARCHAR(50)">
        <constraints nullable="false" unique="false"/>
      </column>
      <column name="description" type="VARCHAR(250)">
        <constraints nullable="true" unique="false"/>
      </column>
      <column name="ip_address" type="VARCHAR(50)">
        <constraints nullable="false" unique="false"/>
      </column>
      <column name="query" type="VARCHAR(100)">
        <constraints nullable="true" unique="false"/>
      </column>
    </createTable>
  </changeSet>
</databaseChangeLog>
<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
  <changeSet id="04" author="kc1vmz">
    <createTable tableName="report_eoc_contact">
      <column name="report_eoc_contact_id" type="VARCHAR(36)">
        <constraints nullable="false" unique="true" primaryKey="true" primaryKeyName="report_eoc_contactPK"/>
      </column>
      <column name="callsign" type="VARCHAR(20)">
        <constraints nullable="false" unique="false"/>
      </column>
      <column name="director_name" type="VARCHAR(25)">
        <constraints nullable="true" unique="false"/>
      </column>
      <column name="incident_commander_name" type="VARCHAR(25)">
        <constraints nullable="true" unique="false"/>
      </column>
      <column name="reported_date" type="DATETIME">
        <constraints nullable="false"/>
      </column>
    </createTable>
    <addColumn tableName="report_eoc_mobilization">
      <column name="eoc_name" type="VARCHAR(30)" defaultValue=" ">
          <constraints nullable="false" unique="false" />
      </column>
    </addColumn>
  </changeSet>
</databaseChangeLog>

<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
  <changeSet id="06" author="kc1vmz">
    <createTable tableName="callsign_ace">
      <column name="callsign_ace_id" type="VARCHAR(36)">
        <constraints nullable="false" unique="true" primaryKey="true" primaryKeyName="callsign_acePK"/>
      </column>
      <column name="callsign_target" type="VARCHAR(20)">
        <constraints nullable="false" unique="false"/>
      </column>
      <column name="callsign_checked_type" type="TINYINT">
        <constraints nullable="false" unique="false"/>
      </column>
      <column name="callsign_checked" type="VARCHAR(20)">
        <constraints nullable="false" unique="false"/>
      </column>
      <column name="allowed" type="TINYINT">
        <constraints nullable="false" unique="false"/>
      </column>
      <column name="proximity" type="TINYINT">
        <constraints nullable="true" unique="false"/>
      </column>
    </createTable>
  </changeSet>
</databaseChangeLog>
